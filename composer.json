{
    "minimum-stability": "stable",
    "name": "radebatz/assetic-utils",
    "description": "Collection of assetic related code.",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "support": {
        "issues": "https://github.com/DerManoMann/assetic-utils/issues",
        "source": "https://github.com/DerManoMann/assetic-utils.git"
    },
    "require": {
        "php": ">=5.6",
        "kriswallsmith/assetic": "^1.3",
        "symfony/filesystem": "~2.8|^3.0",
        "psr/log": "^1.0",
        "psr/cache": "^1.0"
    },
    "require-dev": {
        "silex/silex": "~2.0",
        "twig/twig": "^1.30",
        "symfony/twig-bridge": "^3.2",
        "symfony/yaml": "~2.8|^3.0",
        "monolog/monolog": "^1.21",
        "radebatz/acache": "^1.3",
        "phpunit/phpunit": "~5",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "twig/twig": "^1.30",
        "symfony/twig-bridge": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\Assetic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radebatz\\Assetic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
