{
    "minimum-stability": "stable",
    "name": "radebatz/assetic-utils",
    "description": "Collection of assetic related code.",
    "license": [
        "MIT"
    ],
    "homepage": "http://radebatz.net/mano/",
    "authors": [
        {
            "name": "Martin Rademacher",
            "email": "mano@radebatz.org"
        }
    ],
    "support": {
        "issues": "https://github.com/DerManoMann/assetic-utils/issues",
        "source": "https://github.com/DerManoMann/assetic-utils.git"
    },
    "require": {
        "php": ">=5.6",
        "kriswallsmith/assetic": "^1.3",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4",
        "satooshi/php-coveralls": "dev-master",
        "symfony/yaml": "~2.3|^3.0",
        "monolog/monolog": "^1.21",
        "radebatz/acache": "^1.3"
    },
    "suggest": {
        "psr/cache": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Radebatz\\Assetic\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Radebatz\\Assetic\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
